{"manifest_version": 2, "name": "bytewerk.org collective orders", "version": "2.0", "description": "add a page action to parse shopping carts from various electronics distributors, clipboard-copy the content in mediawiki syntax suitable for the bytewerk.org wiki", "author": "Hubert Denkmair", "permissions": ["clipboardWrite"], "page_action": {"browser_style": true, "default_icon": {"16": "icon/logo-16.png", "19": "icon/logo-19.png", "32": "icon/logo-32.png", "38": "icon/logo-38.png"}, "default_title": "copy cart to clipboard", "show_matches": ["https://www.mouser.de/Cart/*", "https://www.reichelt.de/Warenkorb/*", "https://www.pollin.de/checkout/cart", "https://www.digikey.de/ordering/shoppingcart*", "https://www.conrad.de/de/cart.html"]}, "background": {"scripts": ["background-script.js"]}, "content_scripts": [{"matches": ["https://www.mouser.de/Cart/*"], "js": ["common.js", "mouser.js"]}, {"matches": ["https://www.reichelt.de/Warenkorb/*"], "js": ["common.js", "reichelt.js"]}, {"matches": ["https://www.pollin.de/checkout/cart"], "js": ["common.js", "pollin.js"]}, {"matches": ["https://www.digikey.de/ordering/shoppingcart*"], "js": ["common.js", "digikey.js"]}, {"matches": ["https://www.conrad.de/de/cart.html"], "js": ["common.js", "conrad.js"]}]}