{"name": "ManicTime Url Tracker", "version": "1.1.13", "manifest_version": 2, "description": "Sends urls to local ManicTime installation. Only works if ManicTime is installed locally.", "icons": {"16": "images/icon-16.png", "128": "images/icon-128.png"}, "default_locale": "en", "background": {"scripts": ["scripts/serverManager.js", "scripts/background.js", "scripts/bowser.js"]}, "browser_action": {"default_icon": "images/icon-16.png", "default_title": "ManicTime", "default_popup": "popup.html"}, "permissions": ["tabs"], "browser_specific_settings": {"gecko": {"id": "@manictimeurlextractor"}}}