{"name": "RealDonaldContext", "version": "1.1", "manifest_version": 2, "description": "Adds context to Trump's not-quite-accurate tweets.", "homepage_url": "http://www.washingtonpost.com", "author": "Philip Bump", "browser_action": {"default_icon": {"48": "images/Icon_48.png"}}, "background": {"scripts": ["reload.js"], "persistent": true}, "permissions": ["tabs", "downloads"], "icons": {"128": "images/Icon_128.png"}, "web_accessible_resources": ["images/Logo.png"], "content_security_policy": "script-src 'self' https://www.pbump.net/*; object-src 'self'", "content_scripts": [{"matches": ["*://*.twitter.com/realDonaldTrump/*", "*://*.twitter.com/realdonaldtrump/*", "*://*.twitter.com/realdonaldTrump/*", "*://*.twitter.com/realDonaldtrump/*"], "js": ["reply.js"], "run_at": "document_end"}]}