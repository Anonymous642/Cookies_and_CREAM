{"name": "Comment Skipper", "version": "1.0", "manifest_version": 2, "description": "Skip to the next top level comment in Hacker News", "browser_action": {"default_icon": "images/logo.png", "default_popup": "popup.html"}, "permissions": ["tabs", "activeTab", "declarativeContent"], "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'", "background": {"page": "background.html", "persistent": false}, "content_scripts": [{"js": ["dist/js/content.js"], "matches": ["https://news.ycombinator.com/item*"]}], "commands": {"skip": {"suggested_key": {"default": "Ctrl+Shift+1", "mac": "Command+Shift+1"}, "description": "Skip to next top level comment"}}}