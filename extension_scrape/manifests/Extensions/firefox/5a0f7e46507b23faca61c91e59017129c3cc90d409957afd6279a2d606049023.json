{"manifest_version": 2, "name": "Trinkey-ify Youtube", "version": "0.2", "description": "Adds my cat to all youtube thumbnails", "icons": {"48": "assets/icons/icon-48.png", "96": "assets/icons/icon-96.png"}, "permissions": ["*://www.youtube.com/*", "storage"], "web_accessible_resources": ["assets/images/trinkey/*.png", "assets/images/taylor/*.png"], "content_scripts": [{"matches": ["https://www.youtube.com/*"], "js": ["main.js"]}], "browser_action": {"default_popup": "popup.html"}, "options_ui": {"page": "popup.html", "open_in_tab": false}, "browser_specific_settings": {"gecko": {"id": "youtubecat@trinkey", "strict_min_version": "48.0"}}}