{"manifest_version": 2, "name": "Sked Media Library", "author": "Sked Social", "version": "1.0.7", "description": "Add content to Sked's Media Library.", "background": {"scripts": ["background.js"], "persistent": false}, "content_security_policy": "script-src 'self'; object-src 'self'", "browser_action": {"default_popup": "popup.html", "default_icon": {"16": "images/logo_16.png", "32": "images/logo_32.png", "48": "images/logo_48.png", "128": "images/logo_128.png"}}, "icons": {"16": "images/logo_16.png", "32": "images/logo_32.png", "48": "images/logo_48.png", "128": "images/logo_128.png"}, "web_accessible_resources": ["sharedData.js"], "permissions": ["*://*.skedsocial.com/*", "notifications", "activeTab", "contextMenus", "storage", "cookies"], "content_scripts": [{"matches": ["https://*.instagram.com/*", "https://instagram.com/*"], "js": ["contentScript.js"]}]}