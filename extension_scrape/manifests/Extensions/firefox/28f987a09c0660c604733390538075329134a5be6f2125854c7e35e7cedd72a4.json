{"manifest_version": 2, "name": "Lazygur", "description": "Lets you browse Imgur without having to change pictures manually, just press Shift+Ctrl+Right arrow and relax!", "version": "1", "icons": {"16": "icon16.png", "48": "icon48.png", "128": "icon128.png"}, "browser_action": {"default_icon": "icon128.png"}, "commands": {"toggle-feature": {"suggested_key": {"default": "Ctrl+Shift+Right"}, "description": "Starts and stops the auto browse"}}, "content_scripts": [{"matches": ["*://imgur.com/*"], "js": ["jquery-3.2.1.min.js", "auto.js"]}], "background": {"scripts": ["background.js"], "persistent": false}, "permissions": ["storage"], "web_accessible_resources": ["wheel.png"]}