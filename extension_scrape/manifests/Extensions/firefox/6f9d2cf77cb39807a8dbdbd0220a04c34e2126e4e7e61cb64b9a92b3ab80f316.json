{"manifest_version": 2, "applications": {"gecko": {"id": "youtube-full-video-title@strempfer.works", "strict_min_version": "42.0"}}, "name": "YouTube Full Video Title", "description": "Instead of just the two first lines of a video title, with this extension you will be able to see the entire thing.", "version": "0.1", "browser_action": {"default_icon": "icon128.png", "default_title": "YouTube Full Video Title"}, "icons": {"16": "icon16.png", "48": "icon48.png", "128": "icon128.png", "512": "icon512.png"}, "content_scripts": [{"matches": ["*://*.youtube.com/*"], "js": ["inject.js"], "css": ["inject.css"], "run_at": "document_start"}]}