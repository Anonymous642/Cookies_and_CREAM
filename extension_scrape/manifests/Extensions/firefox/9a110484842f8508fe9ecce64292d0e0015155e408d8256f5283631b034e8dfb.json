{"manifest_version": 2, "name": "Givero - Search the web to support charities", "version": "1.4.2", "description": "Transform your web searches into a force for good. 20 seconds is all it takes. This extension sets Givero as your default search engine.", "browser_action": {"default_icon": "images/icon_default.png"}, "applications": {"gecko": {"id": "givero@givero.com", "strict_min_version": "53.0"}}, "icons": {"16": "images/givero_16.png", "32": "images/givero_32.png", "48": "images/givero_48.png", "64": "images/givero_64.png", "96": "images/givero_96.png", "128": "images/givero_128.png", "192": "images/givero_192.png", "256": "images/givero_256.png", "512": "images/givero_512.png"}, "background": {"scripts": ["js/main.js"], "persistent": true}, "omnibox": {"keyword": "g"}, "chrome_settings_overrides": {"search_provider": {"name": "Givero", "keyword": "givero", "search_url": "https://www.givero.com/search?q={searchTerms}&src=ext", "favicon_url": "https://www.givero.com/favicon.ico", "is_default": true}}}