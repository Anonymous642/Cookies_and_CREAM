{"name": "hns-online-extension", "version": "0.0.1", "manifest_version": 2, "author": "diateam", "description": "Hns-online can connect to this extension to catch copy on remote view and send it to user clipboard", "browser_action": {"default_title": "hns-online extension", "default_popup": "clipboard.html", "default_icon": {"16": "16HNS_Online.png", "48": "48HNS_Online.png", "128": "128HNS_Online.png"}}, "content_scripts": [{"matches": ["https://*.hns-platform.com/*", "http://localhost/*"], "js": ["content.js"]}], "permissions": ["clipboardRead", "clipboardWrite", "tabs", "webNavigation"], "background": {"scripts": ["background.js"]}}