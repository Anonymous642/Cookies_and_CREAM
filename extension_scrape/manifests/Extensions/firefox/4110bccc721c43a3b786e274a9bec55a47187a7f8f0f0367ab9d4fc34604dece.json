{"name": "Lazy Search on Youtube", "version": "0.9", "manifest_version": 2, "description": "Simple and quick way to search for highlighted text on YouTube via right-click context menu or from popup. YouTube Helper.", "homepage_url": "https://github.com/allekss90/", "permissions": ["contextMenus", "tabs", "*://clients1.google.com/complete/search*"], "background": {"scripts": ["background.js"]}, "options_ui": {"page": "./options/options.html", "open_in_tab": false}, "browser_action": {"default_popup": "./popup/popup.html", "default_title": "Lazy Search on Youtube", "browser_style": false, "default_icon": {"16": "images/ico16.png", "32": "images/ico32.png", "48": "images/ico48.png", "128": "images/ico128.png"}}, "icons": {"16": "images/ico16.png", "32": "images/ico32.png", "48": "images/ico48.png", "128": "images/ico128.png"}, "commands": {"toggle-feature": {"suggested_key": {"default": "Ctrl+Shift+Y", "linux": "Ctrl+Shift+Y"}, "description": "Send a 'toggle-feature' event to the extension"}}}