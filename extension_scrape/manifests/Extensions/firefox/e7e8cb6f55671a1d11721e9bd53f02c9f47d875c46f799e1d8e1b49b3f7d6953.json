{"name": "SDR Web Radio", "version": "0.6.0", "manifest_version": 2, "description": "SDR Web Radio extension, a WebSDR companion", "icons": {"128": "images/icono128.png"}, "browser_action": {"default_icon": "images/icono.png", "default_title": "SDR Web Radio"}, "content_scripts": [{"matches": ["http://*/*", "https://*/*"], "include_globs": ["*:8901*", "*:8902*", "*:8903*", "*websdr*"], "exclude_globs": ["https://sdrwebradio.000webhostapp.com/", "http://*websdr.org/*", "https://ve3sun.com/*", "http://www.dxsummit.fi/*", "https://sdr.hu/"], "js": ["content_scripts/jquery.js", "content_scripts/injector.js"]}], "web_accessible_resources": ["images/*.jpeg", "images/*.png"]}