{"manifest_version": 2, "name": "YouTube Subscriber", "description": "Subscribe to YouTube channels without needing a Google account.", "version": "0.4.0", "author": "Ole Bl\u00e4sing", "homepage_url": "https://gitlab.com/oleblaesing/youtube-subscriber", "icons": {"48": "icon.svg", "96": "icon.svg"}, "permissions": ["storage", "https://www.youtube.com/feeds/videos.xml", "https://*.ytimg.com/*"], "background": {"scripts": ["src/modules/fetch-videos.js", "src/background.js"]}, "browser_action": {"default_title": "YouTube Subscriber", "default_popup": "src/popup/popup.html", "default_icon": {"16": "icon.svg", "32": "icon.svg", "64": "icon.svg"}}, "content_scripts": [{"matches": ["https://www.youtube.com/*"], "js": ["src/content.js"]}], "options_ui": {"page": "src/options/options.html"}}