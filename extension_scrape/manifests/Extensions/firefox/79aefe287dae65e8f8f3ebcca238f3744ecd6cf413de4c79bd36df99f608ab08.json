{"manifest_version": 2, "name": "Geoguessr Plus", "description": "This extension adds extra accessability functions to geoguessr.", "version": "2.3.1", "author": "Brennan McDonald", "browser_specific_settings": {"gecko": {"id": "ggp@ebrennan.sh", "strict_min_version": "42.0"}}, "icons": {"48": "img/icon.png"}, "browser_action": {"default_title": "Geoguessr+", "default_icon": {"19": "img/icon.png", "20": "img/icon.png", "38": "img/icon.png"}, "default_popup": "ui/settings.html"}, "web_accessible_resources": ["css/darkmode.css"], "content_scripts": [{"matches": ["*://*.geoguessr.com/*", "*://geoguessr.com/*"], "js": ["darkmode.js", "addheader.js"]}, {"matches": ["*://*.geoguessr.com/battle-royale/*", "*://geoguessr.com/battle-royale/*", "*://*.geoguessr.com/battle-royale", "*://geoguessr.com/battle-royale"], "js": ["battleroyale.js"], "css": ["css/tooltip.css"]}, {"matches": ["*://*.geoguessr.com/me/current", "*://geoguessr.com/me/current"], "js": ["deleteallongoing.js"]}], "permissions": ["storage", "activeTab"]}