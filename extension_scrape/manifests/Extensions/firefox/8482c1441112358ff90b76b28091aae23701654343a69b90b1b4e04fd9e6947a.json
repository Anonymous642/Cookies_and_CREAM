{"manifest_version": 2, "name": "Qwant search engine adder", "description": "It adds the qwant search engine to firefox without any popup.", "version": "1.0", "chrome_settings_overrides": {"homepage": "https://www.qwant.com/", "search_provider": {"name": "Qwant", "keyword": "q", "search_url": "https://www.qwant.com/?q={searchTerms}", "favicon_url": "https://www.qwant.com/favicon.ico", "suggest_url": "https://api.qwant.com/api/suggest/?q={searchTerms}&client=opensearch&lang=__MSG_locale__", "is_default": true}}}