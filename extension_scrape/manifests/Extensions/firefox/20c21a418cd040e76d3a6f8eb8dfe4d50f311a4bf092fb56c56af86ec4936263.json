{"manifest_version": 2, "name": "Alexa Quick Search", "version": "1.2", "description": "Alexa Quick Search access your search engines in a popup panel when you select text in a webpage. Context search menu also included!", "icons": {"48": "icons/icon48.png", "96": "icons/icon96.png"}, "background": {"scripts": ["quickSearch.js"]}, "applications": {"gecko": {"id": "aryan.satish16@gmail.com"}}, "options_ui": {"browser_style": true, "page": "settings/settings.html"}, "permissions": ["<all_urls>", "activeTab", "clipboardWrite", "contextMenus", "storage", "tabs", "downloads", "webNavigation"], "commands": {"open-popup": {"suggested_key": {"default": "Ctrl+Shift+Space"}, "description": "Key combination to open popup"}, "toggle-auto-popup": {"suggested_key": {"default": "Ctrl+Shift+U"}, "description": "Key combination to toggle auto popup opening"}}, "web_accessible_resources": ["res/*"]}