{"name": "CENTPAGE", "description": "This extension automatically sets CENTPAGE.com as your homepage and new tab page.", "manifest_version": 2, "version": "1.0.1", "homepage_url": "http://centpage.com", "icons": {"16": "favicon.png", "32": "icon32.png", "48": "icon48.png"}, "applications": {"gecko": {"strict_min_version": "54.0"}}, "background": {"scripts": ["background.js"]}, "browser_action": {"default_icon": {"16": "icon16.png"}, "default_title": "CENTPAGE"}, "chrome_url_overrides": {"newtab": "redirect.html"}, "permissions": ["tabs", "activeTab", "cookies", "storage"], "content_scripts": [{"matches": ["*://*.centpage.com/*"], "all_frames": false, "run_at": "document_end"}]}