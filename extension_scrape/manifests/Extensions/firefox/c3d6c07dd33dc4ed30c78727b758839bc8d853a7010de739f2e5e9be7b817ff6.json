{"manifest_version": 2, "name": "Livestream downloader", "short_name": "livestream dl", "description": "This extension will allow you to download videos from past events on livestream.com", "version": "1.5.2", "applications": {"gecko": {"id": "livestreamdownloader@sudoplz.com"}}, "permissions": ["tabs", "*://*/*", "downloads"], "content_scripts": [{"matches": ["http://www.livestream.com/*"], "js": ["js/script.js"], "run_at": "document_end"}], "icons": {"16": "img/icon16.png", "48": "img/icon48.png", "128": "img/icon128.png"}, "browser_action": {"default_icon": {"19": "img/icon48.png", "38": "img/icon48.png"}, "default_popup": "popup.html"}, "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'", "web_accessible_resources": ["img/icon128.png"]}