{"manifest_version": 2, "name": "Trady Lens", "version": "1.0", "description": "Stock & Crypto charts right at your tooltip.", "content_security_policy": "script-src 'self'; object-src 'self' img-src 'self' https://tradytics.com/static/assets/images/", "icons": {"48": "figs/circle-icon.png", "64": "figs/circle-icon.png", "92": "figs/circle-icon.png"}, "permissions": ["activeTab", "contextMenus", "tabs", "*://tradytics.com/*"], "web_accessible_resources": ["figs/.png"], "browser_action": {"default_icon": "figs/circle-icon.png", "default_title": "Trady Lens", "default_popup": "popup.html"}, "content_scripts": [{"matches": ["*://*.twitter.com/*", "*://*.facebook.com/*", "*://*.youtube.com/*"], "js": ["jquery-3.3.1.min.js", "popper.min.js", "bootstrap.min.js", "lightweight-charts.standalone.production.js", "tradyview.js", "charting.js", "communication.js"], "css": ["bootstrap.min.css", "custom_css.css"], "all_frames": true, "run_at": "document_start"}]}