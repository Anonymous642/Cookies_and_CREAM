{"manifest_version": 3, "name": "Hide YouTube Cards", "version": "1.0", "description": "Hides YouTube cards at the end of videos", "permissions": ["scripting"], "host_permissions": ["https://www.youtube.com/*", "https://*.youtube.com/*"], "browser_specific_settings": {"gecko": {"id": "hide-youtube-cards@example.com", "strict_min_version": "67.0"}}, "action": {"default_popup": "popup.html", "default_icon": {"16": "icon16.png", "48": "icon48.png", "128": "icon128.png"}}, "icons": {"16": "icon16.png", "48": "icon48.png", "128": "icon128.png"}, "content_scripts": [{"matches": ["*://*.youtube.com/*"], "js": ["content.js"]}]}