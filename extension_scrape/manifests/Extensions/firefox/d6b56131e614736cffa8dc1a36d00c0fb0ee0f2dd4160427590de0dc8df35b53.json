{"name": "Speed Translate", "description": "This addon provides quick translation of selected texts", "version": "2.5", "manifest_version": 2, "icons": {"16": "icons/icon16.png", "48": "icons/icon48.png", "128": "icons/icon128.png"}, "options_ui": {"page": "options.html"}, "content_scripts": [{"all_frames": true, "matches": ["http://*/*", "https://*/*", "*://*/*"], "css": ["css/translate_popup.css"], "js": ["js/common/saferInnerHTML.js", "js/common/common.js", "js/common/jquery-3.4.1.js", "js/common/jquery.draggable.js", "js/common/languages.js", "js/contentscript/reminderhelper.js", "js/contentscript/translate.js", "js/contentscript/main.js"], "run_at": "document_end"}], "background": {"scripts": ["js/common/common.js", "js/background/reminder.js", "js/background/googlespeak.js", "js/background/background.js"]}, "web_accessible_resources": ["icons/*", "css/*"], "browser_action": {"default_icon": {"19": "icons/icon19.png", "38": "icons/icon38.png"}, "default_title": "Speed Translate", "default_popup": "popup.html"}, "permissions": ["http://*/*", "https://*/*", "*://*/*", "storage"], "applications": {"gecko": {"id": "@speedtranslate", "strict_min_version": "57.0"}}, "content_security_policy": "script-src 'self'; style-src 'self' 'unsafe-inline'; font-src 'self'; object-src 'self'; frame-src 'self'", "commands": {"_execute_browser_action": {"suggested_key": {"default": "Ctrl+Q"}}}}