{"manifest_version": 2, "name": "itdock", "version": "3.0", "homepage_url": "http://www.itdock.in", "description": "Hide all major urls no tabs no right click .. secure your browser  ", "icons": {"48": "icons/icon-48.png"}, "applications": {"gecko": {"id": "care@itdock.in"}}, "browser_action": {"browser_style": true, "default_popup": "popup/disable.html", "default_icon": {"16": "icons/iconoff-48.png", "32": "icons/iconoff-32.png"}}, "background": {"scripts": ["background.js"]}, "content_scripts": [{"matches": ["<all_urls>"], "js": ["program.js"]}], "permissions": ["<all_urls>", "tabs", "activeTab", "webRequest", "webRequestBlocking", "contextMenus", "notifications", "webNavigation", "history", "storage", "alarms"]}