{"manifest_version": 2, "name": "Larry Filter", "version": "2.5.1", "description": "Larry Filter will hide tweets containing a list of words, #hashtags, @usernames or advertising on Twitter.com!", "icons": {"16": "images/Twitter-16.png", "32": "images/Twitter-32.png", "48": "images/Twitter-48.png", "128": "images/Twitter-128.png"}, "applications": {"gecko": {"id": "larry@larryfilter.com", "strict_min_version": "48.0"}}, "content_scripts": [{"css": ["twitter.css"], "js": ["jquery.js", "larryfilter.js"], "matches": ["http://twitter.com/*", "https://twitter.com/*", "http://www.twitter.com/*", "https://www.twitter.com/*"], "run_at": "document_end"}], "browser_action": {"browser_style": false, "default_icon": "images/Twitter-32.png", "default_popup": "menu.html"}, "background": {"scripts": ["background.js"]}, "permissions": ["tabs", "storage", "http://twitter.com/*", "https://twitter.com/*", "http://www.twitter.com/*", "https://www.twitter.com/*"], "default_locale": "en"}