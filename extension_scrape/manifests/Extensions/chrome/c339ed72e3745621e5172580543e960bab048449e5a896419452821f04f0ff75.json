{"update_url": "https://clients2.google.com/service/update2/crx", "name": "PCI Pal Agent Assist Session Code to Talkdesk", "version": "1.0.0", "manifest_version": 3, "description": "This extension is used to detect a new PCI Pal Agent assist iframe's session code and provide it to Talkdesk while on a call.", "homepage_url": "https://talkdesk.com", "icons": {"16": "icons/icon16.png", "48": "icons/icon48.png", "128": "icons/icon128.png"}, "background": {"service_worker": "src/bg/background.js"}, "content_scripts": [{"matches": ["https://*.pcipal.cloud/*"], "js": ["src/inject/pcipal.js"], "all_frames": true}, {"matches": ["https://*.talkdesk.com/*"], "js": ["src/inject/talkdesk.js"], "all_frames": true}]}