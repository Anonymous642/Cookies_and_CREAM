{"update_url": "https://clients2.google.com/service/update2/crx", "manifest_version": 2, "name": "URSS Feeds Discovery", "description": "Discovers RSS feeds advertised on webpages and allow to open them directly. Requires the app \"URSS Feeds Reader\" to be installed.", "author": "Jereck.NET Consulting", "homepage_url": "https://www.jereck.net/Consulting/Projects/URSS-Feeds-Reader/", "version": "1.0.2", "icons": {"16": "img/icon_16.png", "32": "img/icon_32.png", "48": "img/icon_48.png", "128": "img/icon_128.png"}, "permissions": ["http://*/*", "https://*/*", "storage", "nativeMessaging"], "background": {"scripts": ["js/discovery.js", "js/background.js"], "persistent": false}, "page_action": {"default_icon": {"16": "img/no_feeds_16.png", "24": "img/no_feeds_24.png", "32": "img/no_feeds_32.png"}, "default_popup": "html/popup.html", "default_title": "No RSS feeds found on this page"}, "content_scripts": [{"js": ["js/discovery.js", "js/document_start.js"], "matches": ["http://*/*", "https://*/*"], "run_at": "document_start"}, {"js": ["js/discovery.js", "js/document_end.js"], "matches": ["http://*/*", "https://*/*"], "run_at": "document_end"}]}