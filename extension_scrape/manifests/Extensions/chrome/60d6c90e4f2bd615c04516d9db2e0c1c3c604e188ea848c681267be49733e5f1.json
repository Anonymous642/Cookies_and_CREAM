{"update_url": "https://clients2.google.com/service/update2/crx", "name": "Gather Mute", "short_name": "GatherMute", "author": "Luiggi Reffatti", "version": "1.0.0", "manifest_version": 3, "description": "Easy muting/unmuting for when you're using Gather Town.", "icons": {"32": "icons/icon32.png", "48": "icons/icon48.png", "128": "icons/icon128.png"}, "default_locale": "en", "background": {"service_worker": "js/background.js"}, "content_scripts": [{"matches": ["https://app.gather.town/*"], "js": ["js/gathermute.js"]}], "action": {"default_icon": "icons/icon32.png", "default_title": "Disconnected"}, "host_permissions": ["https://app.gather.town/*"], "commands": {"toggle_mute": {"description": "Toggle Mute", "suggested_key": {"default": "Ctrl+Shift+K"}, "global": true}, "mute": {"description": "Mute", "suggested_key": {"default": "Ctrl+Shift+M"}, "global": true}, "unmute": {"description": "Unmute", "suggested_key": {"default": "Ctrl+Shift+B"}, "global": true}, "ptt": {"description": "Push to Talk", "suggested_key": {"default": "Ctrl+Shift+L"}, "global": true}}}