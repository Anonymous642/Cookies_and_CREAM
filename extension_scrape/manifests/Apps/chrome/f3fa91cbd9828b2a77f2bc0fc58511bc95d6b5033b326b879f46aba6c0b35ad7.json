{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "eSentral eBook Reader",
  "description": "Your ebook portal",
  "version": "1.2",
  "manifest_version": 2,
  "offline_enabled": true,
  "icons": {
    "16": "esentral.png",
    "48": "esentral.png",
    "128": "esentral.png"
  },
  "homepage_url": "http://reader.e-sentral.com",
  "app": {
    "launch": {
      "local_path": "cloudreader.html"
    }
  },
  "permissions": [
      "unlimitedStorage"
  ]
}
